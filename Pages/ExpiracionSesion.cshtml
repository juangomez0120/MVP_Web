@page
@model MVP_Web.Pages.ExpiracionSesionModel
@{
    ViewData["Title"] = "Expiración";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en" style="height:100vh">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Sprint3</title>

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" />

    <!-- Font -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Oswald:wght@500&display=swap" rel="stylesheet">

</head>
<body id="bodyLogin">
    <header>
    </header>

    <main role="main">
        <div class="row" style="font-family:'Oswald', sans-serif;">
            <div class="contenido col s12 m5 l4">
                <div class="row">
                    <img id="logoTerniumLogin" src="~/img/logoTernium.png" class="col s8 m5 l5" />
                </div>
                <h2 class="texto center-align" style="font-family:'Oswald', sans-serif;">El Gran Escape</h2>
                <div class="row">
                    <p class="center-align">El tiempo de tu sesión se ha agotado.</p>
                </div>
                <div class="row">
                    <a href="Index" type="button" id="botonlogin" class="btn center-align">Reiniciar Sesión</a>
                </div>
            </div>
        </div>

    </main>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
</body>
</html>
